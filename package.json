{
  "name": "go-figure",
  "version": "0.1.0",
  "homepage": "./",
  "author": "Tim Portfolio",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@material-ui/core": "^4.12.3",
    "@material-ui/icons": "^4.11.2",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "blob-util": "^2.0.2",
    "classnames": "^2.3.1",
    "concurrently": "^5.3.0",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^1.2.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.7.1",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-dropzone": "^10.2.2",
    "react-hotkeys": "^2.0.0",
    "react-hotkeys-hook": "^1.6.1",
    "react-redux": "^7.2.5",
    "react-router-cache-route": "^1.11.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "3.4.0",
    "react-selectable-fast": "^3.4.0",
    "react-sketch": "^0.5.1",
    "recharts": "^2.1.2",
    "redux": "^4.1.1",
    "wait-on": "^4.0.2"
  },
  "devDependencies": {
    "electron": "^14.0.0",
    "electron-builder": "^22.11.7",
    "electron-packager": "^15.3.0"
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "webpack.config.js"
    },
    "renderer": {
      "webpackConfig": "webpack.config.js"
    }
  },
  "browser": {
    "fs": false
  },
  "build": {
    "appId": "io.timportfol.go-figure",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "files": [
    "build",
    "*.js",
    "public"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "electron-dev": "concurrently  \"SET BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
