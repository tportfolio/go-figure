{
  "name": "go-figure",
  "version": "1.0.0",
  "homepage": "./",
  "author": "Tim Portfolio",
  "private": true,
  "main": "public/electron.js",
  "dependencies": {
    "@emotion/react": "^11.4.1",
    "@emotion/styled": "11.3.0",
    "@mui/icons-material": "^5.0.0",
    "@mui/material": "^5.0.0",
    "@mui/styles": "^5.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/user-event": "^13.2.1",
    "blob-util": "^2.0.2",
    "classnames": "^2.3.1",
    "concurrently": "^6.2.1",
    "cross-env": "^7.0.3",
    "electron-is-dev": "^2.0.0",
    "lodash": "^4.17.21",
    "loglevel": "^1.7.1",
    "moment": "^2.29.1",
    "re-resizable": "^6.9.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-dropzone": "^11.4.2",
    "react-hotkeys": "^2.0.0",
    "react-hotkeys-hook": "^1.6.1",
    "react-redux": "^7.2.5",
    "react-router-cache-route": "^1.11.1",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^5.0.1",
    "react-selectable-fast": "^3.4.0",
    "react-sketch": "npm:react-sketch2@^0.5.7",
    "recharts": "^2.1.2",
    "redux": "^4.1.1",
    "wait-on": "^6.0.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-react": "^7.14.5",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "electron": "^26.2.4",
    "electron-builder": "^24.13.3",
    "electron-packager": "^17.1.2",
    "enzyme": "^3.11.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.0",
    "jest-environment-jsdom": "^27.2.4"
  },
  "overrides": {
    "nth-check": "2.1.1"
  },
  "electronWebpack": {
    "main": {
      "webpackConfig": "webpack.config.js"
    },
    "renderer": {
      "webpackConfig": "webpack.config.js"
    }
  },
  "browser": {
    "fs": false
  },
  "build": {
    "appId": "io.timportfol.go-figure",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  },
  "files": [
    "build",
    "*.js",
    "public"
  ],
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "preelectron-pack": "npm run build",
    "electron-pack": "electron-builder -c.extraMetadata.main=build/electron.js",
    "electron-dev": "concurrently  \"SET BROWSER=none&&npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "postinstall": "electron-builder install-app-deps",
    "test": "jest",
    "eject": "react-scripts eject"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "./src/setupTests.js"
    ],
    "testEnvironment": "jsdom"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not ie <= 11",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
